{% extends "admin.html.twig" %}



{% block body %}
   <section class="card">
        <div class="card-block">
            <h5 class="with-border m-t-lg">Ajouter champs</h5>

            <div class="row">
                {{ form_start(form, {'attr': {'class': 'form-control'}}) }}
					<div class="col-lg-4 float">
						<fieldset class="form-group">
							{{ form_row(form.nomChamps) }}
						</fieldset>
					</div>
                    <div class="clear"></div>
                    <div class="col-lg-12">
						<fieldset class="form-group">
							<input type="submit" value="Create" class="btn btn-inline"/>
						</fieldset>
					</div>
				
                    {{ form_end(form) }}
				</div>

        </div>
	</section>
   <ul>
        <li>
            <a href="{{ path('champs_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
